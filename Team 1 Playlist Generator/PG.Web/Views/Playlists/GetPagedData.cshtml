@model IPagedList<PlaylistViewModel>
@{
    ViewBag.Title = "Home Page";
}





@foreach (var item in Model)
{
    <p>@item.Title</p>
}

@for (int i = 0; i < Model.PageCount; i++)
{
    var num = i + 1;
    <a asp-controller="Playlists" asp-action="GetPagedData" asp-route-pageNumber="@num"> @num </a>
}
